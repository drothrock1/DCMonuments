                            <state key="normal" title="Jan Scruggs, 1982" image="Jan Scruggs, 1982.png">
                                <color key="titleColor" red="0.1960784314" green="0.30980392159999998" blue="0.52156862749999999" alpha="1" colorSpace="calibratedRGB"/>
